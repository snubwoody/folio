{
    "name": "@folio/desktop",
    "version": "1.2.0",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "tauri": "tauri",
        "db:sync": "cd src-tauri && sqlx database drop -y && sqlx database create && sqlx migrate run",
        "test": "vitest --browser=chromium",
        "test:browser": "vitest --browser=chromium",
        "test:headless": "vitest --browser.headless run",
        "test:unit": "vitest --project unit run"
    },
    "dependencies": {
        "@internationalized/date": "^3.10.0",
        "@tauri-apps/api": "^2",
        "@tauri-apps/plugin-opener": "^2",
        "@tauri-apps/plugin-os": "~2",
        "date-fns": "^4.1.0"
    },
    "devDependencies": {
        "@tauri-apps/cli": "^2"
    }
}
